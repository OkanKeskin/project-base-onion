{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection" : "*"
  },
  "JwtSettings": {
    "SecretKey": "*",
    "Issuer": "*",
    "Audience": "*",
    "AccessTokenExpirationMinutes": 5,
    "RefreshTokenExpirationDays": 1
  },
  "SmtpSettings": {
    "Server": "smtp.gmail.com",
    "Port": 587,
    "EnableSsl": true,
    "Username": "*",
    "Password": "*",
    "SenderEmail": "*",
    "SenderName": "*",
    "UseDefaultCredentials": false,
    "LogInsteadOfSend": true
  },
  "AWS": {
    "AccessKey": "*",
    "SecretKey": "*",
    "Region": "*",
    "BucketName": "*"
  },
  "Application": {
    "FrontendUrl": "*"
  },
  "AllowedHosts": "*"
}
